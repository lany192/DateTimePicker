<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lany.picker.samples">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DatePickerActivity"></activity>
        <activity android:name=".TimePickerActivity"></activity>
        <activity android:name=".LanyPickerActivity"></activity>
        <activity android:name=".CalendarViewActivity"></activity>
        <activity android:name=".YmdhPickerActivity"></activity>
    </application>

</manifest>
